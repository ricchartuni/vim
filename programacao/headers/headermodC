   static char *mylocal_errtype(const int itype, char *cerr){
//---------------------------------------------------------------------
      switch(itype){
         default:
            strcpy(cerr, CERR_NOTDEFINED);
            break;
      }
      return cerr;
   }
//======================================================================
//======================================================================
   static void mylocal_print(XXXXXXX_str st){
//---------------------------------------------------------------------
      //----------------------------------------------------------
      MYLOG_INFOHDR("Input information\n");
      //----------------------------------------------------------
      MYLOG_INFOPRN("nsamples ..... %8d\n", 999);
      //----------------------------------------------------------
      MYLOG_INFOEND();
      //----------------------------------------------------------
   }
