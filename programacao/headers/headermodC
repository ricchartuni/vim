   XXXXXXX_str XXXXXXX_null(){
//---------------------------------------------------------------------
//  Do not need to change ............................................
      XXXXXXX_str st;
      mylocal_null(&st);
      return st;
   }
//======================================================================
//======================================================================
   static int mylocal_errmain(XXXXXXX_str *st, const int iret, const int itype){
//---------------------------------------------------------------------
//  Do not need to change ............................................
      if(iret == 0) return 0;
      // ------------------------
      // Found error ......
      // ------------------------
      char cerr[1024];
      mylocal_errtype(itype, cerr);
      MYLOG_ERR(itype, "%s\n", cerr);
      XXXXXXX_free(st);
      return 1;
   }
